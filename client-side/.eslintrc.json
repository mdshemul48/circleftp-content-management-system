{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": [2, { "allow": ["_id"] }],
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto"
            }
          ],
          "react/prop-types": 0,
          "jsx-a11y/label-has-associated-control" : 0
    }
    
}